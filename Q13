#include <stdio.h>

#define MAX 100  

int hashTable[MAX];
int m;          
int hashFunction(int key) {
    return key % m;
}

void insert(int key) {
    int index = hashFunction(key);
    while (hashTable[index] != -1) {
        index = (index + 1) % m;
    }

    hashTable[index] = key;
}
void display() {
    int i;
    printf("\nHash Table:\n");
    for (i = 0; i < m; i++) {
        if (hashTable[i] != -1)
            printf("Location %d : %d\n", i, hashTable[i]);
        else
            printf("Location %d : Empty\n", i);
    }
}

int main() {
    int n, key, i;

    printf("Enter size of hash table (m): ");
    scanf("%d", &m);
    for (i = 0; i < m; i++) {
        hashTable[i] = -1;
    }

    printf("Enter number of employee records (N): ");
    scanf("%d", &n);

    printf("Enter %d employee keys (4-digit):\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &key);
        insert(key);
    }

    display();

    return 0;
}
